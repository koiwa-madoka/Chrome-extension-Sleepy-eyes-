{
    "name": "Blur&Eye for Twitter",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Twitterを開いたまま一定時間が経つとwindow内がぼやけたりPCがうとうとしたりします。twitterに夢中になって長時間使用しないための拡張機能です。",
    "icons": {
      "16": "eye_1.png",
      "48": "eye_2.png",
      "128": "eye_3.png"
    },
    "content_scripts": [{
      "matches": ["https://twitter.com/*"],
      "js": ["jquery-3.5.1.min.js","content.js","lottie.min.js"],
      "css": ["main.css"]
    }],
    "web_accessible_resources": ["eye.json","nc113119.mp3"]
}
  